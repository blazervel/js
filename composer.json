{
  "name": "blazervel/blazervelql",
  "description": "Query language API and JS helper for Laravel",
  "license": "MIT",
  "authors": [
      {
          "name": "Joshua Anderton",
          "email": "hey@joshuaanderton.ca"
      }
  ],
  "require": {
      "php": "^8.1",
      "illuminate/support": "^8.67|^9.0",
      "tightenco/ziggy": "^1.0",
      "lorisleiva/laravel-actions": "^2.4"
  },
  "require-dev": {
      "phpunit/phpunit": "^9.3",
      "phpstan/phpstan": "^1.7",
      "laravel/pint": "^1.1"
  },
  "autoload": {
      "psr-4": {
          "Blazervel\\BlazervelQL\\": "src"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Blazervel\\BlazervelQL\\Tests\\": "tests"
      }
  },
  "config": {
      "sort-packages": true
  },
  "extra": {
      "laravel": {
          "providers": [
              "Blazervel\\BlazervelQL\\Providers\\ServiceProvider"
          ]
      }
  }
}